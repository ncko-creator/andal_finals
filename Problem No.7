import java.util.Scanner;

public class prob7 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int sum = 0;
        int input;

        System.out.println("Enter numbers (0 to stop):");

        while (true) {
            input = sc.nextInt();
            if (input == 0) break;
            sum += input;
        }

        System.out.println("Total Sum: " + sum);
    }
}
