import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter number: ");
        int num = sc.nextInt();
        int count = 0;

        do {
            num /= 10;
            count++;
        } while (num != 0);

        System.out.println("Digits: " + count);
    }
}
