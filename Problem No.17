public class Prob17 {
public static void main (String args []) {
int[] arr = {10, 5, 20, 20, 4};
int max = Integer.MIN_VALUE;
int secMax = Integer.MIN_VALUE;

for (int n : arr) {
    if (n > max) {
        secMax = max;
        max = n;
    } else if (n > secMax && n != max) {
        secMax = n;
    }
}
System.out.println("Second highest: " + secMax);
     }
  }
